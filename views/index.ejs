<body>
    <div id="wrapper">
        <div class="header">
            <nav class="navbar  fixed-top navbar-site navbar-light bg-light navbar-expand-md" role="navigation">
                <div class="container">
                    <div class="navbar-identity">
                        <a href="/" class="navbar-brand logo logo-title">
                            <img class="userImg" src="/images/logo.png" alt="LOGO" style="width:300px; height:61px; border:0px"> </a>

                        <button data-target=".navbar-collapse" data-toggle="collapse" class="navbar-toggler pull-right" type="button">
                            <svg xmlns="http://www.w3.org/2000/svg" viewbox="0 0 30 30" width="30" height="30" focusable="false">
                                <title>Menu</title>
                                <path stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-miterlimit="10" d="M4 7h22M4 15h22M4 23h22" />
                            </svg>
                        </button>
                    </div>

                    <div class="navbar-collapse collapse">
                        <ul class="nav navbar-nav ml-auto navbar-right">
                            <li class="nav-item"><a href="/" class="nav-link"><i class="fa fa-home"></i> Home</a></li>
                            <li class="nav-item"><a href="/accountmanager" class="nav-link"><i class="fa fa-users"></i> Account Manager</a></li>
                            <li class="nav-item"><a href="/register" class="nav-link"><i class="fa fa-edit"></i> Register</a></li>
                            <li class="nav-item"><a href="/login" class="nav-link"><i class="fas fa-sign-in-alt"></i> Login</a></li>
                            <li class="postadd nav-item"><a class="btn btn-block btn-border btn-post btn-success nav-link" href="/listing">Post a Listing</a>
                            </li>
                        </ul>
                    </div>
                </div>
                <!-- /.container-fluid -->
            </nav>
        </div>
        <!-- /.header -->

        <div class="intro" style="background-image: url(images/index6.jpg);">
            <div class="dtable hw100">
                <div class="dtable-cell hw100">
                    <div class="container text-center">
                        <h1 class="intro-title animated fadeInDown"> Empowering Your Journey with Premium Online Accounts. </h1>

                        <p class="sub animateme fittext3 animated fadeIn">
                            Crafting Your Path to Excellence, Account by Account
                        </p>
                        <form action="#" method="GET">
                            <div class="row search-row animated fadeInUp">
                                <div class="col-md-8">
                                    <input class="form-control keyword" type="search" name="q" value="" placeholder="search here by typing a keyword e.g. 'Remotasks'">
                                </div>
                                <div class="col-md-3">
                                    <button class="btn btn-block btn-primary  "> <i class="fa fa-search"></i> Search
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.intro -->

        <div class="main-container">
            <div class="container">

                <div class="col-xl-12 content-box ">
                    <div class="row row-featured row-featured-category">
                        <div class="col-xl-12  box-title no-border">
                            <div class="inner">
                                <h2><span>Browse by</span>
                                    Category <a href="#" class="sell-your-item"> All listings <i class="  fa fa-bars"></i> </a></h2>
                            </div>
                        </div>

                        <div class="col-xl-2 col-md-4 col-sm-4 col-xs-6 f-category">
                            <a href="#"><i class="fa-solid fa-bars  fa-6x"></i>
                                <h6> All Listings </h6></a>
                        </div>
                        <div class="col-xl-2 col-md-4 col-sm-4 col-xs-6 f-category">
                            <a href="#"><i class="fa fa-graduation-cap fa-6x"></i>
                                <h6> Microservices </h6></a>
                        </div>
                        <div class="col-xl-2 col-md-4 col-sm-4 col-xs-6 f-category">
                            <a href="#"><i class="fa-solid fa-pen-to-square fa-6x" aria-hidden="true"></i>
                                <h6> Writing </h6></a>
                        </div>
                        <div class="col-xl-2 col-md-4 col-sm-4 col-xs-6 f-category">
                            <a href="#"><i class="fa fa-headphones fa-6x"></i>
                                <h6> Transcription </h6></a>
                        </div>
                        <div class="col-xl-2 col-md-4 col-sm-4 col-xs-6 f-category">
                            <a href="/zoom"><i class="fa fa-video fa-6x"></i>
                                <h6> Online Training </h6></a>
                        </div>
                        <div class="col-xl-2 col-md-4 col-sm-4 col-xs-6 f-category">
                            <a href="#"><i class="fa fa-plus fa-6x"></i>
                                <h6> Consulatation </h6></a>
                        </div>
                    </div>
                </div>

                <div style="clear: both"></div>
                <div class="row">
                    <div class="col-md-9 page-content col-thin-right">
                        <div class="row">
                            <div class="col-md-12 page-content col-thin-left">
                                <div class="category-list ">
                                    <div class="tab-box "></div>
                                    <!--/.tab-box-->

                                    <div class="listing-filter">
                                        <div class="pull-left col-xs-6">
                                            <div class="breadcrumb-list"><a href="#" class="current"> <span>All Listings</span></a>
                                            </div>
                                        </div>
                                        <div style="clear:both"></div>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <% listings.forEach(listing => { %>
                            <div class="item-list">
                                <div class="row">
                                    <div class="col-md-2 no-padding photobox">
                                        <div class="add-image">
                                            <h3><%= listing.title %></h3>
                                            <h4><%= listing.description %></h4>

                                            <h4>Type: <%= listing.type %></h4>
                                            
                                        </div>
                                    </div>
                                    <!--/.photobox-->
                                    <div class="col-md-7 add-desc-box">
                                        <div class="ads-details">
                                            <h3 class="add-title">
                                                <a href="add"><%= listing.title %></a>
                                            </h3>
                                            <span class="info-row">
                                                <span class="date"><i class="fas fa-clock"> </i> Listed <%= listing.time %> ago by
                                                    <a href="#"><%= listing.author %></a>
                                                </span>
                                            </span>
                                        </div>
                                    </div>
                                    <!--/.add-desc-box-->
                                    <div class="col-md-3 text-right price-box">
                                        <h2 class="item-price"> <%= listing.price %> </h2>
                                        <a href="/add/<%= listing._id %>" class="btn btn-success btn-sm make-favorite">View Ad</a>
                                    </div>
                                    
                                    <!--/.add-desc-box-->
                                </div>
                            </div>
                            <!--/.item-list-->
                        <% }); %>
                        
                        



                    </div>

                    <!--sidebar content -->
                    <div class="col-md-3 sidebar">
                        <div class="sidebar-content">
                            <h3>Adverts</h3>
                            <div style="width:100%;height:0;padding-bottom:100%;position:relative;"><iframe src="https://giphy.com/embed/vSFQi6ZYy5bIOZztQk" width="100%" height="100%" style="position:absolute" frameBorder="0" class="giphy-embed" allowFullScreen></iframe></div><p><a href="https://giphy.com/gifs/online-shopping-vemobro-philippines-vSFQi6ZYy5bIOZztQk"></a></p>
                             
                        </div>
                    </div>


                </div>

            </div>

        </div>


    </div>
</body>

